#xfce-mcs-manager &

sh $HOME/.screenlayout/4m.sh &
compton -b --config $HOME/.config/compton.conf &
feh --bg-fill $HOME/.wallpaper.jpg &
tint2 &
xscreensaver -no-splash &
volumeicon &
conky -q -c $HOME/.config/conky/conky.conf &
conky -q -c $HOME/.config/conky/conky2.conf &
